{
  "name": "optimistdigital/nova-multiselect-field",
  "description": "A multiple select field for Laravel Nova.",
  "keywords": [
    "laravel",
    "nova",
    "multiselect",
    "select"
  ],
  "authors": [
    {
      "name": "Tarvo Reinpalu",
      "email": "tarvoreinpalu@gmail.com",
      "role": "Developer"
    },
    {
      "name": "Optimist Digital",
      "email": "info@optimistdigital.com",
      "role": "Maintainer"
    }
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^4.0",
    "optimistdigital/nova-translations-loader": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "OptimistDigital\\MultiselectField\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "OptimistDigital\\MultiselectField\\FieldServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
